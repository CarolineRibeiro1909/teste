<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>My CRUD in NodeJS</title>
</head>

<body>

    <h2>Registros
        <h2>
            <table border="1">
                <thead>
                    <tr>
                        <td>Nome</td>
                        <td>Sobrenome</td>
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach(function(details) { %>
                        <tr>
                            <td>
                                <%= details.nome %>
                            </td>
                            <td>
                                <%= details.sobrenome %>
                            </td>
                            <td>
                                <a href="/editar/<%=details._id %>">Editar</a>
                            </td>
                            <td>
                                <a href="/deletar/<%=details._id %>">Excluir</a>
                            </td>
                        </tr>
                        <% }) %>

                </tbody>
                <button><a href="/">Voltar</a></button>
</body>

</html>